var row;
var col;

for (row = 1; row <= 7; row++) {
    for (col = 1; col <= 5; col++) {
        if (((col == 1 || col == 5) && (row != 1 && row != 7)) || ((row == 1 || row == 7) && (col < 5)))
            document.write("*");
        else
            document.write("&nbsp;" + "&nbsp;")

    }
    document.write("<br>");


}
